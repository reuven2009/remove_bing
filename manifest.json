{
    "name": "Microsoft Bing Remover",
    "description": "Removes the Microsoft Bing search engine from Edge.",
    "icons": {
        "16": "icons/16x16.png",
        "32": "icons/32x32.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },

    "version": "1.1.0.0",
    "manifest_version": 2,

    "content_scripts": [{
        "matches": [
            "https://www.bing.com/search*"
        ],
        "js": [
            "lib/script.js"
        ]
    }],
    "browser_action": {
        "default_popup": "popup/popup.html"
    }
}